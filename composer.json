{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3.0",
        "arxeiss/passwordrule": "1.0.2",
        "bacon/bacon-qr-code": "1.0.3",
        "barryvdh/laravel-debugbar": "^3.5",
        "bigit/vokativ": "dev-master",
        "erusev/parsedown-extra": "^0.8.1",
        "fideloper/proxy": "4.4.1",
        "guzzlehttp/guzzle": "7.2.0",
        "laravel/framework": "8.26.1",
        "laravel/sanctum": "^2.9",
        "laravel/ui": "3.2.0",
        "mpdf/mpdf": "8.0.10",
        "phpoffice/phpspreadsheet": "1.16.0",
        "prologue/alerts": "0.4.8",
        "queueworker/sansdaemon": "1.2.3",
        "resampler/resampler": "^1.0",
        "tamtamchik/namecase": "2.2.0"
    },
    "require-dev": {
        "arxeiss/coding-standards": "^0.7.0",
        "facade/ignition": "2.5.10",
        "fzaninotto/faker": "1.9.2",
        "laravel/tinker": "2.6.0",
        "mockery/mockery": "1.4.2",
        "nunomaduro/collision": "5.3.0",
        "nunomaduro/larastan": "0.7.0",
        "php-parallel-lint/php-parallel-lint": "1.2.0",
        "phpunit/phpunit": "9.5.2",
        "heroku/heroku-buildpack-php": "*"
    },
    "autoload": {
        "files": ["app/helpers.php"],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "CzechitasApp\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "pre-autoload-dump": [
            "Google\\Task\\Composer::cleanup"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover"
        ],
        "post-install-cmd": [
            "php artisan migrate --force",
            "php artisan db:seed",
            "npm install",
            "npm run build"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
